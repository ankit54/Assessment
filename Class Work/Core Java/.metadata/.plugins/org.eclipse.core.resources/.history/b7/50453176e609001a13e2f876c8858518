package program5;

import java.util.ArrayList;
import java.util.Collections;

import program1.Animal;
import program1.Cow;
import program1.Lion;

public class CountObjects {
	public static void main(String[] args) {
		Animal c1=new Cow();
		Animal c2=new Cow();
		Animal l1=new Lion();
		Animal l2=new Lion();
		Animal l3=new Lion();
		
		ArrayList<Animal> al=new ArrayList<Animal>();
		al.add(c1);
		al.add(c2);
		al.add(l1);
		al.add(l2);
		al.add(l3);
		
		for (Animal animal : al) {
			System.out.println(Collections.frequency(al, animal));
		}
		
	}
}
